<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/a3login"
    android:background="@drawable/background_login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.A3Login">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jua"
        android:text="Logea y empieza a"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jua"
        android:text="transferir!"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.02" />

    <LinearLayout
        android:id="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:layout_width="411dp"
        android:layout_height="230dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="170dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="75dp"
            android:fontFamily="@font/jua"
            android:text="Email"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="205dp" />


        <EditText
            android:id="@+id/editTextText"
            android:layout_width="351dp"
            android:layout_height="47dp"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/txt_blanco"
            android:fontFamily="@font/jua"
            android:inputType="text"
            android:paddingStart="14sp"
            android:text="Ingresar Email"
            android:textColor="#B5B5B5"
            android:textSize="12sp"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="239dp" />


        <TextView
            android:id="@+id/txtpass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="30dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/jua"
            android:text="Contrase単a"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="33dp"
            tools:layout_editor_absoluteY="316dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/input_pass_login_page"
            app:endIconMode="password_toggle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtpass">

            <com.google.android.material.textfield.TextInputEditText

                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/txt_blanco"
                android:fontFamily="@font/jua"
                android:inputType="textVisiblePassword"
                android:paddingStart="14sp"
                android:paddingEnd="197sp"
                android:text="Ingresar tu contrase単a"
                android:textColor="#B5B5B5"
                android:textSize="14sp" />
        </com.google.android.material.textfield.TextInputLayout>


    </LinearLayout>

    <TextView
        android:id="@+id/txt_olvidaste_contrase単a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.0"
        android:layout_width="175dp"
        android:layout_height="22dp"
        android:fontFamily="@font/jua"
        android:text="Olvidaste tu contrase単a?"
        android:textColor="#1A87DD"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="474dp" />

    <Button
        android:id="@+id/btn_logeo"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.714"
        android:layout_width="347dp"
        android:layout_height="wrap_content"
        android:background="@drawable/boton_celeste"
        android:text="Login"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="619dp" />

    <TextView
        android:id="@+id/txt_crear_cuenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/jua"
        android:text="Crear una nueva cuenta"
        android:textColor="#1A87DD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_logeo" />


</androidx.constraintlayout.widget.ConstraintLayout>